<%= form_with(model: @user, url: users_path) do |form| %>
    <%= form.label :imię %>
    <%= form.text_field :name %>
    
    <%= form.label :nazwisko %>
    <%= form.text_field :last_name %>
    
    <%= form.label :email %>
    <%= form.email_field :email %>
    
    <%= form.submit "Dodaj użytkownika" %>
  <% end %>